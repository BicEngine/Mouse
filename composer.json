{
    "name": "bic-engine/mouse",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "bic-engine/event-contracts": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Bic\\Mouse\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.3",
        "vimeo/psalm": "^5.15"
    },
    "autoload-dev": {
        "psr-4": {
            "Bic\\Mouse\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev",
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "platform-check": true,
        "bin-compat": "full",
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
